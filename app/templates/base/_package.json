{
  "name": "pixel2html-<%= clientId %>-<%= projectId %>",
  "description": "pixel2html <%= clientId %>/<%= projectId %>",
  "license": "MIT",
  "repository": "http://www.pixel2html.com",
  "version": "1.0.0",
  "scripts": {
    "start": "npm install <% if (markupIntegration=='jekyll'){%>&& bundle install<%}%>",
    "code": "gulp",
    "build": "gulp release"
  },
  "dependencies": {
    "del": "^2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "browser-sync": "^2.18.6",
    <% if(frontEndFramework == 'bootstrap'){ %>
    "bootstrap": "4.0.0-alpha.6",
    <% } -%>
    <% if(frontEndFramework == 'foundation'){ %>
    "foundation-sites": "^6.3.0",
    <% } %>
    <% if(frontEndFramework == 'basscss'){ %>
      "basscss-sass": "^4.0.0",
    <% } %>
    <% if(jQuery){ %>
      "jquery": "^3.1.1",
    <% } %>
    <% if (cssProcessor === 'scss' || frontEndFramework) { -%>
    "gulp-sass": "^3.1.0",
    <% } -%>
    <% if (cssProcessor === 'less') { -%>
    "gulp-less": "^3.3.0",
    <% } -%>
    <% if (cssProcessor === 'styl') { -%>
    "gulp-stylus": "^2.6.0",
    <% } -%>

    <% if (markupLanguage === 'pug') { %>
      "gulp-pug": "^3.1.0",
    <% } %>
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "require-dir": "^0.3.0",
    "gulp-zip": "^3.2.0"
  }
}
