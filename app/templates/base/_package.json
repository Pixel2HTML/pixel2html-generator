{
  "name": "<%= projectName %>",
  "description": "<%= projectName %> front end source code",
  "license": "MIT",
  "repository": "http://www.pixel2html.com",
  "version": "1.0.0",
  "scripts": {
    "start": "npm install <% if (markupIntegration=='jekyll'){%>&& bundle install<%}%>",
    "code": "NODE_ENV=development gulp",
    "build": "NODE_ENV=production gulp release --prod",
    "lint:js": "standard",
    "lint:scss": "stylelint ./src/styles/**/*.scss",
    "lint": "npm run lint:js; npm run lint:scss",
    "debug": "NODE_ENV=development webpack --config gulp/webpack.config.js --debug",
    "debug:prod": "NODE_ENV=production webpack --config gulp/webpack.config.js --debug",
    "prod": "NODE_ENV=production gulp build --prod",
    "js": "NODE_ENV=development gulp scripts"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/runtime": "^7.0.0-beta.32",
    "babel-loader": "8.0.0-beta.0",
    "babel-preset-timmy": "^0.3.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    <% if(frontEndFramework === 'bootstrap-4'){ -%>
    "bootstrap": "^4.0.0-alpha.6",
    "popper.js": "^1.12.9",
    <% } -%>
    <% if(frontEndFramework === 'bootstrap-3'){ -%>
    "bootstrap-sass": "^3.3.7",
    <% } -%>
    <% if(frontEndFramework === 'foundation'){ -%>
    "foundation-sites": "^6.4.3",
    <% } -%>
    <% if(jQuery || frontEndFramework){ -%>
    "jquery": "^3.2.1",
    <% } -%>
    <% if (markupLanguage === 'pug') { -%>
    "gulp-pug": "^3.3.0",
    "gulp-html-prettify": "^0.0.1",
    <% } -%>
    <% if (markupLanguage === 'html') { -%>
    "gulp-html-replace": "^1.6.2",
    <% } -%>
    "gulp-purifycss": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-zip": "^4.0.0",
    "require-dir": "^0.3.2",
    "sass-module-importer": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "vinyl-ftp": "^0.6.0",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "create-cert-files": "^1.0.2",
    "eslint": "^4.13.1",
    "eslint-config-pixel2html": "^1.0.0",
    "react-dev-utils": "^4.2.1",
    "stylelint": "^8.4.0",
    "stylelint-config-pixel2html": "^1.0.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-monitor": "^1.0.14"
  },
  "stylelint": {
    "extends": "stylelint-config-pixel2html"
  },
  "eslintConfig": {
    "extends": "pixel2html"
  }
}
