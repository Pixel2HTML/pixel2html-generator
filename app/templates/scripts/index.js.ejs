<% if (frontEndFramework) { -%>
// Start frontEndFramework
import './framework'
<% } -%>

// If you DON'T want to try Hot Module HotModuleReplacement
// then delete everything below.
// Learn more: https://www.andrewhfarmer.com/webpack-hmr-tutorial/

// Here we are importing the whole app
// read more about modules here:
// http://wesbos.com/javascript-modules/
import App from './app'

// Run our app code once
App()

// Get HMR on Development
if (module.hot) {
  module.hot.accept('./app', () => {
    const NextApp = require('./app').default
    NextApp()
  })
}
